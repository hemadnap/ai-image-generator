# Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# ============================================================================
# Google OAuth Configuration (REQUIRED for sign-in)
# ============================================================================
# Get your Client ID from: https://console.cloud.google.com/
# Steps: See GOOGLE_AUTH_SETUP.md for complete instructions
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Replicate API Configuration
# REPLICATE_API_TOKEN is REQUIRED for the backend server to work
REPLICATE_API_TOKEN=your_replicate_api_token_here
# VITE_REPLICATE_API_TOKEN is optional for the frontend (only if using direct API calls)
VITE_REPLICATE_API_TOKEN=your_replicate_api_token_here

# Backend API Configuration (REQUIRED for image generation)
# Make sure the backend server is running on this URL
# Do NOT include /api in the baseURL - the backend endpoints already include it
VITE_API_BASE_URL=http://localhost:8000

# App Configuration
VITE_APP_NAME=AI Image Generator
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
